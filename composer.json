    "keywords": ["op3nvoice", "openvoice", "api", "audio search", "video search"],
    "homepage": "https://github.com/caseysoftware/op3nvoice-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Keith Casey",
            "email": "contrib@caseysoftware.com"
        },
        {
            "name": "Luis Cordova",
            "email": "cordoval@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "~4.0.0"
    },
    "require-dev": {
        "raulfraile/ladybug": "dev-master@dev",
        "phpunit/phpunit": "~4.1.0"
